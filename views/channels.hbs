
<div class="channels-table ">
    <h1 class="channels-table-heading" ><span class="channels-table-span">{{name}} channels</span></h1>
    <table id="myTable" class="table  table-responsive table-sm table-striped table-bordered">
        <thead>
            <tr>
                <th> Img </th>
                <th>Title</th>
                <th>Description</th>

                <th>Subscribe</th>
            </tr>
        </thead>
        <tbody>
            {{#each channels}}
            <tr>
                <td class="img-col align-middle"> <img class="table_img" src="{{thumbnail_url}}" height="40px"
                        width="40px" /> </td>
                <td>
                    <a target="_blank" href="https://www.youtube.com/channel/{{channelId}}">  {{title}}   </a>   
                </td>
                <td>
                    {{ description}}
                <td class="align-middle">
                    <button class="sub-btn" id='{{channelId}}' onclick="defineRequest('{{channelId}}')">Subscribe
                    </button>
                    {{!-- <a href="https://www.youtube.com/channel/{{channelId}}?sub_confirmation=1" target="_blank">Subscribe</a>
                    --}}
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>

<script>
    Swal.fire({
        title: '{{name}}',
        text: 'Welcome to the list of my channels',
        imageUrl: '{{imageUrl}}',
        imageWidth: 130,
        imageHeight: 130,
        imageAlt: 'Custom image',
        animation: true
    })

</script>