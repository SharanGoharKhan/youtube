


    <table id="myTable" class="table table-responsive table-sm table-striped table-bordered">
        <thead>
            <tr>
                <th >Title</th>
                <th >Description</th>

                <th >Subscribe</th>
            </tr>
        </thead>
        <tbody>
            {{#each channels}}
            <tr>
                <td>{{title}}</td>
                <td>
                    {{ description}}                    
                <td>
                   <a href="https://www.youtube.com/channel/{{channelId}}?sub_confirmation=1" target="_blank">Subscribe</a>
                </td>
            </tr>
            {{/each}}
        </tbody>
    </table>

